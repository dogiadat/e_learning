<div>
  <h2 class="center" >Create new category</h2>
  <br/>
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= form_for [:admin, @category] do |f| %>
        <%= render "shared/error_messages", object: @category %>
        <%= f.label :name %>
        <%= f.text_field :name %>
        <br/>
        <%= f.submit "Create", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div>
